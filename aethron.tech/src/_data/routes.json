      "nl": "/nl/"
    },
    "/about/": {
      "template": "about",
      "en": "/about/",
      "nl": "/nl/about/"
    },
    "/solutions/": {
      "template": "solutions",
      "en": "/solutions/",
      "nl": "/nl/solutions/"
    },
    "/contact/": {
      "template": "contact",
      "en": "/contact/",
      "nl": "/nl/contact/"
    }
  },
  "seoRoutes": {
    "en": {
      "sitemap": "/sitemap.xml",
      "robots": "/robots.txt"
    },
    "nl": {
      "sitemap": "/nl/sitemap.xml",
      "robots": "/nl/robots.txt"
    }
  }
