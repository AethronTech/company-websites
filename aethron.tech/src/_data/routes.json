    "en": {
      "home": "/",
      "about": "/about/",
      "solutions": "/solutions/",
      "contact": "/contact/"
    },
    "nl": {
      "home": "/nl/",
      "about": "/nl/about/",
      "solutions": "/nl/solutions/",
      "contact": "/nl/contact/"
    }
  },
  "routeMapping": {
    "/": {
      "template": "index",
